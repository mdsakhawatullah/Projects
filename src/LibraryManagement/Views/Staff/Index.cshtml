@model IEnumerable<LibraryManagement.Models.Staff>

@{
	ViewData["Title"] = "Index";
}

<h1>Staf Index</h1>

<table class="table">

	<thead>
		<tr>
			<th>Name</th>
			<th>StaffID</th>
			<th>Position</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Name</td>
				<td>@item.StaffId</td>
				<td>@item.Position</td>

			</tr>
		}
	</tbody>

</table>