@model IEnumerable<LibraryManagement.Models.Category>

@{
	ViewData["Title"] = "Index";
}

<h1>Category Index</h1>

<table class="table">

	<thead>
		<tr>
			<th>Name</th>
			<th>Descriotion</th>
			<th>CreateDate</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Name</td>
				<td>@item.Description</td>
				<td>@item.CreateDate</td>

			</tr>
		}
	</tbody>

</table>