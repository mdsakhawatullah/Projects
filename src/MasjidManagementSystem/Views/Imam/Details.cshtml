@model MasjidManagementSystem.Models.Imam
@{
	ViewData["Title"] = "Details-Imam";
}

<h1>Details</h1>
<h4>Imam</h4>
<hr />
<div class="row">


	<div class="col-sm-2">
		ID: @Model.Id

	</div>
	<div class="col-sm-10">
		Title: @Model.Name
	</div>
	<div class="col-sm-2">
		Date: @Model.HireDate
	</div>
	<div class="col-sm-10">
		Qualification: @Model.Qualification
	</div>

</div>
<div>
	<a asp-action="Update" asp-route-id="@Model.Id">Update</a>
	<a asp-action="Index">Back To List</a>
